{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://e-municipal.com/schemas/geo-point.json",
    "title": "Geo-point",
    "description": "Geo-point object containing geographical coordinates",
    "required": [ "latitude", "longitude" ],
    "type": "object",
    "properties": {
      "latitude": {
        "type": "number",
        "minimum": -90,
        "maximum": 90
      },
      "longitude": {
        "type": "number",
        "minimum": -180,
        "maximum": 180
      }
    }
  }